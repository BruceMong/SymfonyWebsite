{% extends 'base.html.twig' %} 

{% block title %} Admin View Profil {{ app.user.nom }} {% endblock %}

{% block body %}
<section class="wrapper style1 special">
<div class= "infoUser">
<h1>Liste des utilisateurs</h1>
<ul>
    {% for user in users %}
